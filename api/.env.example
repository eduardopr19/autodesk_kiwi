# AutoDesk Kiwi - Configuration Example
# Copiez ce fichier vers .env et configurez selon vos besoins

# ======================
# Application
# ======================
APP_NAME="AutoDesk Kiwi API"
APP_VERSION="1.0.0"
DEBUG=true

# ======================
# Database
# ======================
DATABASE_URL="sqlite:///data.db"

# ======================
# Hyperplanning (optionnel)
# ======================
# URL de votre calendrier iCal Hyperplanning
# Si vous n'utilisez pas Hyperplanning, laissez vide
HYPERPLANNING_URL=""
# Exemple: HYPERPLANNING_URL="https://votre-ecole.fr/calendar.ics"

# ======================
# API Settings
# ======================
USER_AGENT="AutoDeskKiwi/1.0 (kiwi-app-local-dev)"
API_TIMEOUT=12.0

# ======================
# CORS
# ======================
# Origins autorisées pour le frontend (séparées par des virgules)
CORS_ORIGINS=["http://127.0.0.1:8000","http://localhost:8000"]
