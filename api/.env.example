# ===========================
# AutoDesk Kiwi API - Configuration Example
# ===========================
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# Application
APP_NAME="AutoDesk Kiwi API"
APP_VERSION="1.0.0"
DEBUG=true

# Database
# For development: SQLite (default)
DATABASE_URL="sqlite:///data.db"
# For production: PostgreSQL
# DATABASE_URL="postgresql://user:password@localhost:5432/autodesk_kiwi"

# External API Configuration
USER_AGENT="AutoDeskKiwi/1.0 (your-email@example.com)"
API_TIMEOUT=12.0

# Hyperplanning Configuration
# Get your personal iCal URL from Hyperplanning extranet
# Example: https://extranet-hp-cgy.ensup.eu/hp-cgy/Telechargements/ical/Edt_YOURNAME.ics?version=...&icalsecurise=YOUR_TOKEN&param=...
HYPERPLANNING_URL="https://your-hyperplanning-url-here"

# CORS Origins (comma-separated, for development)
# CORS_ORIGINS="http://127.0.0.1:5500,http://localhost:5500,http://127.0.0.1:5173,http://localhost:5173"
